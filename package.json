{
  "name": "mscgenjs",
  "version": "2.0.0-beta-6",
  "description": "Sequence chart rendering library",
  "main": "src/index.js",
  "dependencies": {
    "amdefine": "1.0.1",
    "requirejs": "2.3.5"
  },
  "devDependencies": {
    "almond": "0.3.3",
    "chai": "4.1.2",
    "chai-xml": "0.3.2",
    "dependency-cruiser": "4.0.0",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-security": "1.4.0",
    "jest": "23.0.0",
    "jest-json-schema": "2.0.0",
    "js-makedepend": "3.0.0",
    "jsdom": "11.11.0",
    "lodash-cli": "4.17.5",
    "npm-check-updates": "2.14.2",
    "nsp": "3.2.1",
    "pegjs": "0.10.0"
  },
  "scripts": {
    "depcruise": "depcruise --validate -- src test",
    "depcruise:graph": "depcruise --validate --do-not-follow \"node_modules|lib\" --output-type dot --module-systems cjs --exclude \"amdefine|lodash\" src | dot -T svg > tmp_deps.svg",
    "lint": "eslint src test",
    "lint:fix": "eslint --fix src test",
    "lint:types": "tslint types/mscgen.d.ts",
    "npm-check-updates": "ncu --upgrade",
    "nsp": "nsp check",
    "test": "jest --onlyChanged --collectCoverage false",
    "test:all": "jest --collectCoverage false",
    "test:cover": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mscgenjs/mscgenjs-core"
  },
  "author": "Sander Verweij",
  "license": "GPL-3.0",
  "keywords": [
    "mscgen",
    "sequence chart",
    "sequence diagram",
    "xu",
    "msgenny"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "collectCoverageFrom": [
      "src/index.js",
      "src/index-lazy.js",
      "src/main/**/*.js",
      "src/parse/**/*.js",
      "src/render/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "src/render/graphics/styling",
      "src/render/graphics/svgelementfactory/wobbly\\.js",
      "src/parse/.+parser\\.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 99.8,
        "branches": 92.9,
        "functions": 100,
        "lines": 99.8
      },
      "src/main": {
        "statements": 100,
        "branches": 93.9,
        "functions": 100,
        "lines": 100
      },
      "src/render": {
        "statements": 99.8,
        "branches": 92.8,
        "functions": 100,
        "lines": 99.8
      },
      "src/parse": {
        "statements": 83,
        "branches": 74,
        "functions": 91,
        "lines": 87
      }
    }
  },
  "engines": {
    "node": ">=6.0"
  },
  "types": "./types/mscgen.d.ts",
  "bugs": {
    "url": "https://github.com/mscgenjs/mscgenjs-core/issues"
  },
  "homepage": "https://github.com/mscgenjs/mscgenjs-core"
}
