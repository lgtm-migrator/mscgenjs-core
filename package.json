{
  "name": "mscgenjs",
  "version": "1.15.1",
  "description": "Sequence chart rendering library",
  "main": "index.js",
  "dependencies": {
    "amdefine": "1.0.1",
    "requirejs": "2.3.5"
  },
  "devDependencies": {
    "almond": "0.3.3",
    "chai": "4.1.2",
    "chai-xml": "0.3.2",
    "dependency-cruiser": "3.1.0",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-security": "1.4.0",
    "js-makedepend": "2.4.9",
    "jsdom": "11.7.0",
    "lodash-cli": "4.17.5",
    "mocha": "5.0.5",
    "npm-check-updates": "2.14.1",
    "nsp": "3.2.1",
    "nyc": "11.6.0",
    "pegjs": "0.10.0"
  },
  "scripts": {
    "depcruise": "depcruise --validate -- main parse render *.js",
    "depcruise:graph": "depcruise --validate --output-type dot --module-systems cjs --exclude \"amdefine|lodash\" main parse render *.js | dot -T svg > tmp_deps.svg",
    "lint": "eslint index.js index-lazy.js indexAMD.js main parse render test",
    "lint:fix": "eslint --fix index.js index-lazy.js indexAMD.js main parse render test",
    "lint:types": "tslint types/mscgen.d.ts",
    "npm-check-updates": "ncu --upgrade",
    "nsp": "nsp check",
    "test": "mocha --reporter dot --bail --timeout 6000 --recursive test",
    "test:cover": "nyc --check-coverage npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mscgenjs/mscgenjs-core"
  },
  "author": "Sander Verweij",
  "license": "GPL-3.0",
  "keywords": [
    "mscgen",
    "sequence chart",
    "sequence diagram",
    "xu",
    "msgenny"
  ],
  "nyc": {
    "statements": 86.64,
    "branches": 77.52,
    "functions": 95.98,
    "lines": 89.6,
    "exclude": [
      "dist/**/*",
      "lib/**/*",
      "indexAMD.js",
      "doc/**/*",
      "coverage/**/*",
      "render/graphics/styling/**",
      "render/graphics/svgelementfactory/wobbly.js",
      "test/**/*"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "engines": {
    "node": ">=4.0"
  },
  "types": "./types/mscgen.d.ts",
  "bugs": {
    "url": "https://github.com/mscgenjs/mscgenjs-core/issues"
  },
  "homepage": "https://github.com/mscgenjs/mscgenjs-core"
}
